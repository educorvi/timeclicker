{
  "name": "timeclicker_client",
  "version": "1.4.1",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "licenses": "license-report --only=prod > public/licenses.json && license-downloader --source ./public/licenses.json --licDir public/license-files --download",
    "generateIcons": "npx vue-pwa-asset-generator --no-manifest -a src/assets/appIcon.png -o public/icons -b \"#FFFFFF\""
  },
  "dependencies": {
    "@vue/compat": "^3.2.45",
    "axios": "^1.2.2",
    "bootstrap": "^4.6.2",
    "bootstrap-vue": "^2.23.1",
    "bootswatch": "^4.6.2",
    "file-saver": "^2.0.5",
    "humanize-duration": "^3.28.0",
    "keycloak-js": "^20.0.3",
    "pinia": "^2.0.28",
    "showdown": "^2.1.0",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/github": "^8.0.7",
    "@types/file-saver": "^2.0.5",
    "@types/humanize-duration": "^3.27.1",
    "@types/node": "^18.11.12",
    "@types/showdown": "^2.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-sfc": "^3.2.45",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "license-downloader": "^1.0.3",
    "license-report": "^6.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sass": "^1.58.3",
    "semantic-release": "^20.1.0",
    "semantic-release-monorepo": "^7.0.5",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vite-plugin-pwa": "^0.14.4",
    "vue-tsc": "^1.0.12"
  }
}

{
  "name": "timeclicker_client",
  "version": "1.12.1",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "licenses": "license-report --only=prod > public/licenses.json && license-downloader --source ./public/licenses.json --licDir public/license-files --download",
    "generateIcons": "npx vue-pwa-asset-generator --no-manifest -a src/assets/appIcon.png -o public/icons -b \"#FFFFFF\"",
    "size": "yarn run build && size-limit"
  },
  "dependencies": {
    "@tsconfig/node-lts": "^18.12.5",
    "@vue/compat": "^3.4.19",
    "axios": "^1.6.7",
    "bootstrap": "^4.6.2",
    "bootstrap-vue": "^2.23.1",
    "bootswatch": "^4.6.2",
    "file-saver": "^2.0.5",
    "humanize-duration": "^3.30.0",
    "keycloak-js": "^20.0.5",
    "pinia": "^2.1.6",
    "showdown": "^2.1.0",
    "swagger-ui": "^5.11.7",
    "swagger-ui-dist": "^4.19.1",
    "vue": "^3.4.19",
    "vue-i18n": "^9.9.1",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.7.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/github": "^8.1.0",
    "@semantic-release/npm": "^9.0.2",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "@size-limit/file": "^8.2.6",
    "@size-limit/time": "^8.2.6",
    "@types/file-saver": "^2.0.6",
    "@types/humanize-duration": "^3.27.4",
    "@types/node": "^18.17.7",
    "@types/showdown": "^2.0.6",
    "@types/swagger-ui": "^3.52.0",
    "@types/swagger-ui-dist": "^3.30.1",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/compiler-sfc": "^3.4.15",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.21.1",
    "license-downloader": "^1.0.8",
    "license-report": "^6.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "sass": "^1.70.0",
    "semantic-release": "^22.0.12",
    "semantic-release-monorepo": "^7.0.5",
    "size-limit": "^8.2.6",
    "typescript": "~5.3.3",
    "vite": "^4.5.2",
    "vite-plugin-pwa": "^0.17.5",
    "vue-tsc": "~1.8.27"
  },
  "size-limit": [
    {
      "limit": "512kB",
      "path": [
        "dist/assets/index*",
        "dist/*"
      ]
    }
  ]
}

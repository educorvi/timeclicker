{
  "name": "timeclicker_client",
  "version": "1.9.0",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "licenses": "license-report --only=prod > public/licenses.json && license-downloader --source ./public/licenses.json --licDir public/license-files --download",
    "generateIcons": "npx vue-pwa-asset-generator --no-manifest -a src/assets/appIcon.png -o public/icons -b \"#FFFFFF\"",
    "size": "yarn run build && size-limit"
  },
  "dependencies": {
    "@vue/compat": "^3.2.47",
    "axios": "^1.3.4",
    "bootstrap": "^4.6.2",
    "bootstrap-vue": "^2.23.1",
    "bootswatch": "^4.6.2",
    "file-saver": "^2.0.5",
    "humanize-duration": "^3.28.0",
    "keycloak-js": "^20.0.5",
    "pinia": "^2.0.33",
    "showdown": "^2.1.0",
    "swagger-ui": "^4.18.2",
    "swagger-ui-dist": "^4.18.2",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/github": "^8.0.7",
    "@size-limit/file": "^8.2.4",
    "@size-limit/time": "^8.2.4",
    "@types/file-saver": "^2.0.5",
    "@types/humanize-duration": "^3.27.1",
    "@types/node": "^18.15.11",
    "@types/showdown": "^2.0.0",
    "@types/swagger-ui": "^3.52.0",
    "@types/swagger-ui-dist": "^3.30.1",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.37.0",
    "eslint-plugin-vue": "^9.10.0",
    "license-downloader": "^1.0.3",
    "license-report": "^6.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "sass": "^1.60.0",
    "semantic-release": "^20.1.3",
    "semantic-release-monorepo": "^7.0.5",
    "size-limit": "^8.2.4",
    "typescript": "~4.7.4",
    "vite": "^4.2.1",
    "vite-plugin-pwa": "^0.14.7",
    "vue-tsc": "~1.2.1"
  },
  "size-limit": [
    {
      "limit": "512kB",
      "path": [
        "dist/assets/index*",
        "dist/*"
      ]
    }
  ]
}
